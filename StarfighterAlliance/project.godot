; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="StarfighterAlliance"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DependencyInjectionNode="res://DependencyInjectionNode.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="ignore"

[dotnet]

project/assembly_name="StarfighterAlliance"
project/solution_directory=".."

[editor_plugins]

enabled=PackedStringArray("res://addons/gdUnit4/plugin.cfg")

[gdunit4]

settings/test/test_lookup_folder="IntegrationTests"
settings/test/test_discovery=true
report/verbose_orphans=false
ui/toolbar/run_overall=true
templates/testsuite/CSharpScript="// GdUnit generated TestSuite

using Godot;
using GdUnit3;

namespace ${name_space}
{
	using static Assertions;
	using static Utils;

	[TestSuite]
	public class ${suite_class_name}
	{
		// TestSuite generated from
		private const string sourceClazzPath = \"${source_resource_path}\";

	}
}
"

